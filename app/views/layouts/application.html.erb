<!DOCTYPE html>
<html>
<head>
  <title>Couponing</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="container">

<% if flash[:notice] %>
  <div id="notice">
    <%= flash[:notice]%>
  </div>
<% end %>

<% if flash[:error] %>
  <div id="error">
    <%= flash[:error]%>
  </div>
<% end %>

<%= yield %>

</div>
    
</body>
</html>
